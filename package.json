{
  "name": "typed.ts",
  "version": "2.1.1",
  "description": "Realistic typing simulations in TypeScript",
  "author": {
    "name": "Janik Schumacher (LoaderB0T)",
    "url": "https://awdware.de"
  },
  "homepage": "https://github.com/LoaderB0T/typed.ts",
  "bugs": {
    "url": "https://github.com/LoaderB0T/typed.ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LoaderB0T/typed.ts.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "main": "./lib/cjs/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "yarn eslint ./src/**",
    "test": "jest",
    "prebuild": "yarn rimraf lib",
    "build": "yarn build-esm && yarn build-cjs",
    "build-base": "tsc -p .",
    "build-esm": "yarn build-base --outDir lib/esm",
    "build-cjs": "yarn build-base --outDir lib/cjs --module commonjs && yarn fix-cjs",
    "fix-cjs": "node --loader ts-node/esm ./fix-cjs.ts"
  },
  "license": "MIT",
  "private": false,
  "dependencies": {},
  "devDependencies": {
    "@awdware/eslint-config-base": "^1.0.0",
    "@awdware/eslint-config-nodejs": "^1.0.0",
    "@awdware/prettier-config": "^0.0.2",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.14",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "jest-junit": "^15.0.0",
    "legally": "^3.5.10",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "prettier": "@awdware/prettier-config",
  "keywords": [
    "node",
    "nodejs",
    "typing",
    "typed",
    "typescript",
    "animation",
    "simulation",
    "keyboard",
    "errors",
    "string",
    "writing"
  ]
}
